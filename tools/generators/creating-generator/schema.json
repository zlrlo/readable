{
  "cli": "nx",
  "id": "creating-generator",
  "type": "object",
  "properties": {
    "pageName": {
      "type": "string",
      "description": "page 또는 section 이름 ",
      "$default": {
        "$source": "pageName"
      }
    },
    "name": {
      "type": "string",
      "description": "Library 이름 ",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "type": "string",
      "description": "Library 타입 ",
      "x-prompt": {
        "message": "Which type of library would you like to generate?",
        "type": "list",
        "items": [
          {
            "value": "data-access",
            "label": "Data Access"
          },
          {
            "value": "feature",
            "label": "Feature"
          },
          {
            "value": "state",
            "label": "State Management"
          }
        ]
      }
    }
  },
  "required": ["name", "type"]
}
